{
  "name": "@blackroad-os/pixel-blackroad-metaverse",
  "version": "0.1.0",
  "description": "BlackRoad OS Pixel Metaverse - Bridge server that watches AI agent activity and broadcasts events to a pixel world visualization",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": {
    "pixel-metaverse": "dist/bin/cli.js",
    "blackroad-pixel": "dist/bin/cli.js"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/cli.js",
    "dev": "tsc && node --watch dist/bin/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "metaverse": "npm run build && npm start",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "claude-code",
    "websocket",
    "pixel-world",
    "metaverse",
    "blackroad-os",
    "ai-agents",
    "visualization"
  ],
  "author": "BlackRoad OS Inc. (forked from waynedev9598/pixelhq)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackRoad-OS/pixel-blackroad-metaverse.git"
  },
  "homepage": "https://github.com/BlackRoad-OS/pixel-blackroad-metaverse#readme",
  "bugs": "https://github.com/BlackRoad-OS/pixel-blackroad-metaverse/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "bonjour-service": "^1.2.1",
    "chokidar": "^3.6.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
